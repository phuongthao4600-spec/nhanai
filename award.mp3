    } </span><span><span class="hljs-keyword">else</span></span><span> {
      certificate.</span><span><span class="hljs-property">classList</span></span><span>.</span><span><span class="hljs-title function_">add</span></span><span>(</span><span><span class="hljs-string">"hidden"</span></span><span>);
    }

    </span><span><span class="hljs-comment">// ====== Gửi dữ liệu ======</span></span><span>
    </span><span><span class="hljs-keyword">const</span></span><span> form = </span><span><span class="hljs-keyword">new</span></span><span> </span><span><span class="hljs-title class_">FormData</span></span><span>();
    form.</span><span><span class="hljs-title function_">append</span></span><span>(</span><span><span class="hljs-string">"name"</span></span><span>, name);
    form.</span><span><span class="hljs-title function_">append</span></span><span>(</span><span><span class="hljs-string">"lop"</span></span><span>, lop);
    form.</span><span><span class="hljs-title function_">append</span></span><span>(</span><span><span class="hljs-string">"camon1"</span></span><span>, c1);
    form.</span><span><span class="hljs-title function_">append</span></span><span>(</span><span><span class="hljs-string">"camon2"</span></span><span>, c2);
    form.</span><span><span class="hljs-title function_">append</span></span><span>(</span><span><span class="hljs-string">"camon3"</span></span><span>, c3);
    form.</span><span><span class="hljs-title function_">append</span></span><span>(</span><span><span class="hljs-string">"viectot"</span></span><span>, vt);
    form.</span><span><span class="hljs-title function_">append</span></span><span>(</span><span><span class="hljs-string">"yeuthuong"</span></span><span>, yt);
    form.</span><span><span class="hljs-title function_">append</span></span><span>(</span><span><span class="hljs-string">"hasCertificate"</span></span><span>, isFull ? </span><span><span class="hljs-string">"x"</span></span><span> : </span><span><span class="hljs-string">""</span></span><span>);

    sendSound.</span><span><span class="hljs-property">currentTime</span></span><span> = </span><span><span class="hljs-number">0</span></span><span>;
    sendSound.</span><span><span class="hljs-title function_">play</span></span><span>().</span><span><span class="hljs-title function_">catch</span></span><span>(</span><span><span class="hljs-function">()=&gt;</span></span><span>{});

    </span><span><span class="hljs-title function_">fetch</span></span><span>(</span><span><span class="hljs-variable constant_">SCRIPT_URL</span></span><span>, {
      </span><span><span class="hljs-attr">method</span></span><span>: </span><span><span class="hljs-string">"POST"</span></span><span>,
      </span><span><span class="hljs-attr">body</span></span><span>: form,
      </span><span><span class="hljs-attr">mode</span></span><span>: </span><span><span class="hljs-string">"no-cors"</span></span><span>
    });

    </span><span><span class="hljs-comment">// ====== Thỏ nói ======</span></span><span>
    </span><span><span class="hljs-title function_">speakAsBunny</span></span><span>(speakText);
  });
});
