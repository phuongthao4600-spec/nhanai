    </span><span><span class="hljs-keyword">return</span></span><span>;
  }

  button.</span><span><span class="hljs-title function_">addEventListener</span></span><span>(</span><span><span class="hljs-string">"click"</span></span><span>, </span><span><span class="hljs-keyword">function</span></span><span> (</span><span><span class="hljs-params">e</span></span><span>) {
    e.</span><span><span class="hljs-title function_">preventDefault</span></span><span>(); </span><span><span class="hljs-comment">// chặn reload trang</span></span><span>

    button.</span><span><span class="hljs-property">disabled</span></span><span> = </span><span><span class="hljs-literal">true</span></span><span>;
    </span><span><span class="hljs-keyword">const</span></span><span> originalText = button.</span><span><span class="hljs-property">innerText</span></span><span>;
