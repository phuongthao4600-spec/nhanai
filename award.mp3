}

</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">handleRequest</span></span><span>(</span><span><span class="hljs-params">e</span></span><span>) {
  </span><span><span class="hljs-keyword">try</span></span><span> {
    </span><span><span class="hljs-comment">// 1. MỞ ĐÚNG BẢNG TÍNH</span></span><span>
    </span><span><span class="hljs-comment">// Cách an toàn nhất: gắn script trực tiếp với sheet rồi dùng getActiveSpreadsheet()</span></span><span>
    </span><span><span class="hljs-keyword">var</span></span><span> ss = </span><span><span class="hljs-title class_">SpreadsheetApp</span></span><span>.</span><span><span class="hljs-title function_">getActiveSpreadsheet</span></span><span>();
    </span><span><span class="hljs-keyword">var</span></span><span> sheet = ss.</span><span><span class="hljs-title function_">getSheets</span></span><span>()[</span><span><span class="hljs-number">0</span></span><span>]; </span><span><span class="hljs-comment">// dùng sheet đầu tiên, hoặc đổi sang getSheetByName("Tên sheet")</span></span><span>

    </span><span><span class="hljs-comment">// 2. LẤY DỮ LIỆU TỪ WEB GỬI LÊN</span></span><span>
    </span><span><span class="hljs-keyword">var</span></span><span> data = e &amp;&amp; e.</span><span><span class="hljs-property">parameter</span></span><span> ? e.</span><span><span class="hljs-property">parameter</span></span><span> : {};

    </span><span><span class="hljs-keyword">var</span></span><span> name = data.</span><span><span class="hljs-property">name</span></span><span> || </span><span><span class="hljs-string">""</span></span><span>;
    </span><span><span class="hljs-keyword">var</span></span><span> lop = data.</span><span><span class="hljs-property">lop</span></span><span> || </span><span><span class="hljs-string">""</span></span><span>;
    </span><span><span class="hljs-keyword">var</span></span><span> camon1 = data.</span><span><span class="hljs-property">camon1</span></span><span> || </span><span><span class="hljs-string">""</span></span><span>;
    </span><span><span class="hljs-keyword">var</span></span><span> camon2 = data.</span><span><span class="hljs-property">camon2</span></span><span> || </span><span><span class="hljs-string">""</span></span><span>;
    </span><span><span class="hljs-keyword">var</span></span><span> camon3 = data.</span><span><span class="hljs-property">camon3</span></span><span> || </span><span><span class="hljs-string">""</span></span><span>;
    </span><span><span class="hljs-keyword">var</span></span><span> viectot = data.</span><span><span class="hljs-property">viectot</span></span><span> || </span><span><span class="hljs-string">""</span></span><span>;
    </span><span><span class="hljs-keyword">var</span></span><span> yeuthuong = data.</span><span><span class="hljs-property">yeuthuong</span></span><span> || </span><span><span class="hljs-string">""</span></span><span>;
    </span><span><span class="hljs-keyword">var</span></span><span> hasCertificate = data.</span><span><span class="hljs-property">hasCertificate</span></span><span> || </span><span><span class="hljs-string">""</span></span><span>; </span><span><span class="hljs-comment">// "x" nếu đủ 3–1–1</span></span><span>

    </span><span><span class="hljs-comment">// 3. GHI THÊM 1 DÒNG MỚI</span></span><span>
    sheet.</span><span><span class="hljs-title function_">appendRow</span></span><span>([
      </span><span><span class="hljs-keyword">new</span></span><span> </span><span><span class="hljs-title class_">Date</span></span><span>(),  </span><span><span class="hljs-comment">// Thời gian gửi</span></span><span>
      name,
      lop,
      camon1,
      camon2,
      camon3,
      viectot,
      yeuthuong,
      hasCertificate
    ]);

    </span><span><span class="hljs-comment">// 4. TRẢ KẾT QUẢ</span></span><span>
    </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-title class_">ContentService</span></span><span>
      .</span><span><span class="hljs-title function_">createTextOutput</span></span><span>(</span><span><span class="hljs-title class_">JSON</span></span><span>.</span><span><span class="hljs-title function_">stringify</span></span><span>({ </span><span><span class="hljs-attr">result</span></span><span>: </span><span><span class="hljs-string">"success"</span></span><span> }))
      .</span><span><span class="hljs-title function_">setMimeType</span></span><span>(</span><span><span class="hljs-title class_">ContentService</span></span><span>.</span><span><span class="hljs-property">MimeType</span></span><span>.</span><span><span class="hljs-property">JSON</span></span><span>);

  } </span><span><span class="hljs-keyword">catch</span></span><span> (err) {
    </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-title class_">ContentService</span></span><span>
      .</span><span><span class="hljs-title function_">createTextOutput</span></span><span>(</span><span><span class="hljs-title class_">JSON</span></span><span>.</span><span><span class="hljs-title function_">stringify</span></span><span>({ </span><span><span class="hljs-attr">result</span></span><span>: </span><span><span class="hljs-string">"error"</span></span><span>, </span><span><span class="hljs-attr">message</span></span><span>: </span><span><span class="hljs-title class_">String</span></span><span>(err) }))
      .</span><span><span class="hljs-title function_">setMimeType</span></span><span>(</span><span><span class="hljs-title class_">ContentService</span></span><span>.</span><span><span class="hljs-property">MimeType</span></span><span>.</span><span><span class="hljs-property">JSON</span></span><span>);
  }
}
