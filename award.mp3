    }
    </span><span><span class="hljs-keyword">if</span></span><span> (!lop) {
      </span><span><span class="hljs-title function_">alert</span></span><span>(</span><span><span class="hljs-string">"Con hãy chọn lớp của mình nhé."</span></span><span>);
      </span><span><span class="hljs-keyword">return</span></span><span>;
    }

    </span><span><span class="hljs-keyword">let</span></span><span> missing = [];
    </span><span><span class="hljs-keyword">if</span></span><span> (!c1) missing.</span><span><span class="hljs-title function_">push</span></span><span>(</span><span><span class="hljs-string">"lời cảm ơn 1"</span></span><span>);
    </span><span><span class="hljs-keyword">if</span></span><span> (!c2) missing.</span><span><span class="hljs-title function_">push</span></span><span>(</span><span><span class="hljs-string">"lời cảm ơn 2"</span></span><span>);
    </span><span><span class="hljs-keyword">if</span></span><span> (!c3) missing.</span><span><span class="hljs-title function_">push</span></span><span>(</span><span><span class="hljs-string">"lời cảm ơn 3"</span></span><span>);
    </span><span><span class="hljs-keyword">if</span></span><span> (!vt) missing.</span><span><span class="hljs-title function_">push</span></span><span>(</span><span><span class="hljs-string">"một việc tốt"</span></span><span>);
    </span><span><span class="hljs-keyword">if</span></span><span> (!yt) missing.</span><span><span class="hljs-title function_">push</span></span><span>(</span><span><span class="hljs-string">"một lời yêu thương"</span></span><span>);

    </span><span><span class="hljs-keyword">const</span></span><span> isFull311 = missing.</span><span><span class="hljs-property">length</span></span><span> === </span><span><span class="hljs-number">0</span></span><span>;

    </span><span><span class="hljs-comment">// ---------- Thỏ trả lời ----------</span></span><span>
    chatBox.</span><span><span class="hljs-property">innerHTML</span></span><span> = </span><span><span class="hljs-string">""</span></span><span>;

    </span><span><span class="hljs-keyword">const</span></span><span> hello = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">createElement</span></span><span>(</span><span><span class="hljs-string">"p"</span></span><span>);
    hello.</span><span><span class="hljs-property">textContent</span></span><span> = </span><span><span class="hljs-string">`Thỏ Nhân Ái chào bạn <span class="hljs-subst">${name}</span></span></span><span> (lớp </span><span><span class="hljs-subst">${lop}</span></span><span>) 🐰`;
    chatBox.</span><span><span class="hljs-title function_">appendChild</span></span><span>(hello);

    </span><span><span class="hljs-keyword">let</span></span><span> replyText = </span><span><span class="hljs-string">`Thỏ Nhân Ái chào bạn <span class="hljs-subst">${name}</span></span></span><span> lớp </span><span><span class="hljs-subst">${lop}</span></span><span>. `;

    </span><span><span class="hljs-keyword">if</span></span><span> (isFull311) {
      </span><span><span class="hljs-keyword">const</span></span><span> praise = </span><span><span class="hljs-title function_">pickUniqueMessage</span></span><span>(praiseMessages, usedPraiseIndexes);
      </span><span><span class="hljs-keyword">const</span></span><span> msg = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">createElement</span></span><span>(</span><span><span class="hljs-string">"p"</span></span><span>);
      msg.</span><span><span class="hljs-property">textContent</span></span><span> = praise + </span><span><span class="hljs-string">" 🌸"</span></span><span>;
      chatBox.</span><span><span class="hljs-title function_">appendChild</span></span><span>(msg);
      replyText += praise;
    } </span><span><span class="hljs-keyword">else</span></span><span> {
      </span><span><span class="hljs-keyword">const</span></span><span> missingText = missing.</span><span><span class="hljs-title function_">join</span></span><span>(</span><span><span class="hljs-string">", "</span></span><span>);

      </span><span><span class="hljs-keyword">const</span></span><span> intro = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">createElement</span></span><span>(</span><span><span class="hljs-string">"p"</span></span><span>);
      intro.</span><span><span class="hljs-property">textContent</span></span><span> =
        </span><span><span class="hljs-string">"Con đã viết rất tốt rồi, nhưng tuần này phiếu 3–1–1 của con vẫn chưa đủ:"</span></span><span>;
      chatBox.</span><span><span class="hljs-title function_">appendChild</span></span><span>(intro);

      </span><span><span class="hljs-keyword">const</span></span><span> ul = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">createElement</span></span><span>(</span><span><span class="hljs-string">"ul"</span></span><span>);
      missing.</span><span><span class="hljs-title function_">forEach</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">m</span></span></span><span>) =&gt; {
        </span><span><span class="hljs-keyword">const</span></span><span> li = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">createElement</span></span><span>(</span><span><span class="hljs-string">"li"</span></span><span>);
        li.</span><span><span class="hljs-property">textContent</span></span><span> = m.</span><span><span class="hljs-title function_">charAt</span></span><span>(</span><span><span class="hljs-number">0</span></span><span>).</span><span><span class="hljs-title function_">toUpperCase</span></span><span>() + m.</span><span><span class="hljs-title function_">slice</span></span><span>(</span><span><span class="hljs-number">1</span></span><span>);
        ul.</span><span><span class="hljs-title function_">appendChild</span></span><span>(li);
      });
      chatBox.</span><span><span class="hljs-title function_">appendChild</span></span><span>(ul);

      </span><span><span class="hljs-keyword">let</span></span><span> template = </span><span><span class="hljs-title function_">pickUniqueMessage</span></span><span>(
        encourageMessagesTemplates,
        usedEncourageIndexes
      );
      </span><span><span class="hljs-keyword">const</span></span><span> encourage = template.</span><span><span class="hljs-title function_">replace</span></span><span>(</span><span><span class="hljs-string">"__MISSING__"</span></span><span>, missingText);

      </span><span><span class="hljs-keyword">const</span></span><span> note = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">createElement</span></span><span>(</span><span><span class="hljs-string">"p"</span></span><span>);
      note.</span><span><span class="hljs-property">textContent</span></span><span> = encourage + </span><span><span class="hljs-string">" 💗"</span></span><span>;
      chatBox.</span><span><span class="hljs-title function_">appendChild</span></span><span>(note);

      replyText += encourage;
    }

    </span><span><span class="hljs-comment">// ---------- Giấy khen ----------</span></span><span>
    </span><span><span class="hljs-keyword">if</span></span><span> (isFull311) {
      certName.</span><span><span class="hljs-property">textContent</span></span><span> = name;
      certificate.</span><span><span class="hljs-property">classList</span></span><span>.</span><span><span class="hljs-title function_">remove</span></span><span>(</span><span><span class="hljs-string">"hidden"</span></span><span>);
      </span><span><span class="hljs-keyword">if</span></span><span> (awardSound) {
        awardSound.</span><span><span class="hljs-property">currentTime</span></span><span> = </span><span><span class="hljs-number">0</span></span><span>;
        awardSound.</span><span><span class="hljs-title function_">play</span></span><span>().</span><span><span class="hljs-title function_">catch</span></span><span>(</span><span><span class="hljs-function">() =&gt;</span></span><span> {});
      }
    } </span><span><span class="hljs-keyword">else</span></span><span> {
      certificate.</span><span><span class="hljs-property">classList</span></span><span>.</span><span><span class="hljs-title function_">add</span></span><span>(</span><span><span class="hljs-string">"hidden"</span></span><span>);
    }

    </span><span><span class="hljs-comment">// ---------- Gửi dữ liệu lên Google Sheet ----------</span></span><span>
    </span><span><span class="hljs-keyword">const</span></span><span> formData = </span><span><span class="hljs-keyword">new</span></span><span> </span><span><span class="hljs-title class_">FormData</span></span><span>();
    formData.</span><span><span class="hljs-title function_">append</span></span><span>(</span><span><span class="hljs-string">"name"</span></span><span>, name);
    formData.</span><span><span class="hljs-title function_">append</span></span><span>(</span><span><span class="hljs-string">"lop"</span></span><span>, lop);
    formData.</span><span><span class="hljs-title function_">append</span></span><span>(</span><span><span class="hljs-string">"camon1"</span></span><span>, c1);
    formData.</span><span><span class="hljs-title function_">append</span></span><span>(</span><span><span class="hljs-string">"camon2"</span></span><span>, c2);
    formData.</span><span><span class="hljs-title function_">append</span></span><span>(</span><span><span class="hljs-string">"camon3"</span></span><span>, c3);
    formData.</span><span><span class="hljs-title function_">append</span></span><span>(</span><span><span class="hljs-string">"viectot"</span></span><span>, vt);
    formData.</span><span><span class="hljs-title function_">append</span></span><span>(</span><span><span class="hljs-string">"yeuthuong"</span></span><span>, yt);
    formData.</span><span><span class="hljs-title function_">append</span></span><span>(</span><span><span class="hljs-string">"hasCertificate"</span></span><span>, isFull311 ? </span><span><span class="hljs-string">"x"</span></span><span> : </span><span><span class="hljs-string">""</span></span><span>);

    </span><span><span class="hljs-keyword">if</span></span><span> (sendSound) {
      sendSound.</span><span><span class="hljs-property">currentTime</span></span><span> = </span><span><span class="hljs-number">0</span></span><span>;
      sendSound.</span><span><span class="hljs-title function_">play</span></span><span>().</span><span><span class="hljs-title function_">catch</span></span><span>(</span><span><span class="hljs-function">() =&gt;</span></span><span> {});
    }

    sendBtn.</span><span><span class="hljs-property">disabled</span></span><span> = </span><span><span class="hljs-literal">true</span></span><span>;
    </span><span><span class="hljs-keyword">const</span></span><span> oldText = sendBtn.</span><span><span class="hljs-property">textContent</span></span><span>;
    sendBtn.</span><span><span class="hljs-property">textContent</span></span><span> = </span><span><span class="hljs-string">"Thỏ đang nhận phiếu..."</span></span><span>;

    </span><span><span class="hljs-title function_">fetch</span></span><span>(</span><span><span class="hljs-variable constant_">SCRIPT_URL</span></span><span>, {
      </span><span><span class="hljs-attr">method</span></span><span>: </span><span><span class="hljs-string">"POST"</span></span><span>,
      </span><span><span class="hljs-attr">body</span></span><span>: formData,
      </span><span><span class="hljs-attr">mode</span></span><span>: </span><span><span class="hljs-string">"no-cors"</span></span><span>
    })
      .</span><span><span class="hljs-title function_">then</span></span><span>(</span><span><span class="hljs-function">() =&gt;</span></span><span> {
        </span><span><span class="hljs-title function_">alert</span></span><span>(</span><span><span class="hljs-string">"Thỏ đã nhận được phiếu 3–1–1 của con rồi! 🧡"</span></span><span>);
      })
      .</span><span><span class="hljs-title function_">catch</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">err</span></span></span><span>) =&gt; {
        </span><span><span class="hljs-variable language_">console</span></span><span>.</span><span><span class="hljs-title function_">error</span></span><span>(</span><span><span class="hljs-string">"Lỗi:"</span></span><span>, err);
        </span><span><span class="hljs-title function_">alert</span></span><span>(</span><span><span class="hljs-string">"Có lỗi khi gửi phiếu. Con báo cô giáo để kiểm tra giúp nhé."</span></span><span>);
      })
      .</span><span><span class="hljs-title function_">finally</span></span><span>(</span><span><span class="hljs-function">() =&gt;</span></span><span> {
        sendBtn.</span><span><span class="hljs-property">disabled</span></span><span> = </span><span><span class="hljs-literal">false</span></span><span>;
        sendBtn.</span><span><span class="hljs-property">textContent</span></span><span> = oldText;
      });

    </span><span><span class="hljs-comment">// ---------- Thỏ nói ----------</span></span><span>
    </span><span><span class="hljs-title function_">speakAsBunny</span></span><span>(replyText);
  });
});
</span></span></code></div></div></pre>
<hr data-start="15831" data-end="15834">
<h2 data-start="15836" data-end="15867">Việc cuối cô cần làm thêm 💡</h2>
<ol data-start="15869" data-end="16185">
<li data-start="15869" data-end="15926">
<p data-start="15872" data-end="15926"><strong data-start="15872" data-end="15912">Dán file <code data-start="15883" data-end="15894">script.js</code> trên vào GitHub</strong> như em nói.</p>
</li>
<li data-start="15927" data-end="16185">
<p data-start="15930" data-end="15988">Thêm <strong data-start="15935" data-end="15958">2 file âm thanh nhẹ</strong> (cô tự chọn mp3 ngắn, dịu):</p>
<ul data-start="15992" data-end="16093">
<li data-start="15992" data-end="16034">
<p data-start="15994" data-end="16034"><code data-start="15994" data-end="16004">send.mp3</code>  – tiếng “ting” nhỏ lúc gửi</p>
</li>
<li data-start="16038" data-end="16093">
<p data-start="16040" data-end="16093"><code data-start="16040" data-end="16051">award.mp3</code> – tiếng reo vui/ting khi hiện Giấy khen</p>
</li>
</ul>
<p data-start="16098" data-end="16185">➜ Cô chỉ cần upload 2 file đó vào cùng thư mục với <code data-start="16149" data-end="16161">index.html</code> (thư mục gốc của site).</p>
</li>
</ol>
<p data-start="16187" data-end="16205">Là xong, học sinh:</p>
<ul data-start="16206" data-end="16413">
<li data-start="16206" data-end="16289">
<p data-start="16208" data-end="16289">Làm <strong data-start="16212" data-end="16218">đủ</strong> → Thỏ khen ngẫu nhiên 40 kiểu + có tiếng giấy khen + hiện Giấy khen.</p>
</li>
<li data-start="16290" data-end="16413">
<p data-start="16292" data-end="16413">Làm <strong data-start="16296" data-end="16307">chưa đủ</strong> → Thỏ hiện danh sách còn thiếu, động viên ngẫu nhiên 40 kiểu, không bắt làm lại, nhắc tuần sau cố gắng.</p>
</li>
