      </span><span><span class="hljs-attr">mode</span></span><span>: </span><span><span class="hljs-string">"no-cors"</span></span><span>
    })
      .</span><span><span class="hljs-title function_">then</span></span><span>(</span><span><span class="hljs-function">() =&gt;</span></span><span> {
