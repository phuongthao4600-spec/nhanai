</span><span><span class="hljs-string">"Chỉ cần thêm __MISSING__ nữa thôi là con hoàn thành tốt rồi. Tuần sau thử lại nha."</span></span><span>,
</span><span><span class="hljs-string">"Tuần này thiếu __MISSING__, nhưng tuần tới con làm trọn vẹn là được."</span></span><span>
];

</span><span><span class="hljs-comment">// ====== TRÁNH TRÙNG LẶP ======</span></span><span>
</span><span><span class="hljs-keyword">let</span></span><span> usedPraiseIndexes = </span><span><span class="hljs-keyword">new</span></span><span> </span><span><span class="hljs-title class_">Set</span></span><span>();
</span><span><span class="hljs-keyword">let</span></span><span> usedEncourageIndexes = </span><span><span class="hljs-keyword">new</span></span><span> </span><span><span class="hljs-title class_">Set</span></span><span>();

</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">pickUniqueMessage</span></span><span>(</span><span><span class="hljs-params">messages, usedSet</span></span><span>) {
  </span><span><span class="hljs-keyword">if</span></span><span> (usedSet.</span><span><span class="hljs-property">size</span></span><span> === messages.</span><span><span class="hljs-property">length</span></span><span>) usedSet.</span><span><span class="hljs-title function_">clear</span></span><span>();
  </span><span><span class="hljs-keyword">let</span></span><span> i;
  </span><span><span class="hljs-keyword">do</span></span><span> {
    i = </span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">floor</span></span><span>(</span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">random</span></span><span>() * messages.</span><span><span class="hljs-property">length</span></span><span>);
  } </span><span><span class="hljs-keyword">while</span></span><span> (usedSet.</span><span><span class="hljs-title function_">has</span></span><span>(i) &amp;&amp; usedSet.</span><span><span class="hljs-property">size</span></span><span> &lt; messages.</span><span><span class="hljs-property">length</span></span><span>);
  usedSet.</span><span><span class="hljs-title function_">add</span></span><span>(i);
  </span><span><span class="hljs-keyword">return</span></span><span> messages[i];
}

</span><span><span class="hljs-comment">// ====== GIỌNG NÓI CỦA THỎ ======</span></span><span>
</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">speakAsBunny</span></span><span>(</span><span><span class="hljs-params">text</span></span><span>) {
  </span><span><span class="hljs-keyword">if</span></span><span> (!(</span><span><span class="hljs-string">"speechSynthesis"</span></span><span> </span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-variable language_">window</span></span><span>)) </span><span><span class="hljs-keyword">return</span></span><span>;
  </span><span><span class="hljs-variable language_">window</span></span><span>.</span><span><span class="hljs-property">speechSynthesis</span></span><span>.</span><span><span class="hljs-title function_">cancel</span></span><span>();
  </span><span><span class="hljs-keyword">const</span></span><span> utter = </span><span><span class="hljs-keyword">new</span></span><span> </span><span><span class="hljs-title class_">SpeechSynthesisUtterance</span></span><span>(text);
  utter.</span><span><span class="hljs-property">lang</span></span><span> = </span><span><span class="hljs-string">"vi-VN"</span></span><span>;
  </span><span><span class="hljs-keyword">const</span></span><span> voices = speechSynthesis.</span><span><span class="hljs-title function_">getVoices</span></span><span>();
  </span><span><span class="hljs-keyword">const</span></span><span> vi = voices.</span><span><span class="hljs-title function_">filter</span></span><span>(</span><span><span class="hljs-function"><span class="hljs-params">v</span></span></span><span> =&gt; v.</span><span><span class="hljs-property">lang</span></span><span>.</span><span><span class="hljs-title function_">startsWith</span></span><span>(</span><span><span class="hljs-string">"vi"</span></span><span>));
  </span><span><span class="hljs-keyword">if</span></span><span> (vi.</span><span><span class="hljs-property">length</span></span><span> &gt; </span><span><span class="hljs-number">0</span></span><span>) utter.</span><span><span class="hljs-property">voice</span></span><span> = vi[</span><span><span class="hljs-number">0</span></span><span>];
  speechSynthesis.</span><span><span class="hljs-title function_">speak</span></span><span>(utter);
}

</span><span><span class="hljs-comment">// ====== ÂM THANH ======</span></span><span>
</span><span><span class="hljs-keyword">let</span></span><span> sendSound = </span><span><span class="hljs-literal">null</span></span><span>;
</span><span><span class="hljs-keyword">let</span></span><span> awardSound = </span><span><span class="hljs-literal">null</span></span><span>;

