</span><span><span class="hljs-keyword">const</span></span><span> </span><span><span class="hljs-variable constant_">SCRIPT_URL</span></span><span> = </span><span><span class="hljs-string">"https://script.google.com/macros/s/AKfycbwPwGNKdT0r0LcmBZKuL3LBBhZPbQSej82JhPDOyhyj0n7DivVuOXVEthYd9tDd7yvfQg/exec"</span></span><span>;

</span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">addEventListener</span></span><span>(</span><span><span class="hljs-string">"DOMContentLoaded"</span></span><span>, </span><span><span class="hljs-keyword">function</span></span><span> (</span><span><span class="hljs-params"></span></span><span>) {
